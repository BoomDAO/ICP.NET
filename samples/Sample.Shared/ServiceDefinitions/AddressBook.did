type Address = record {
  street : text;
  city : text;
  zip_code : nat;
  country : text;
};
type Dict = vec record {
  text;
  nat;
}
service address_book : {
  set_address: (name : text, addr : Address) -> ();
  get_address: (name : text) -> (opt Address) query;
  get_dict: () -> (Dict) query;
}